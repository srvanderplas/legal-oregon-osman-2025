}

@article{kerkhoffDesignResultsExploratory2015,
	title = {Design and results of an exploratory double blind testing program in firearms examination},
	volume = {55},
	issn = {1355-0306},
	url = {https://www.sciencedirect.com/science/article/pii/S1355030615000908},
	doi = {10.1016/j.scijus.2015.06.007},
	abstract = {In 2010, the Netherlands Forensic Institute ({NFI}) and the University of Amsterdam ({UvA}) started a series of tests for the {NFI}'s Firearms Section. Ten cartridge case and bullet comparison tests were submitted by various external parties as regular cases and mixed in the flow of real cases. The results of the tests were evaluated with the {VU} University Amsterdam ({VUA}). A total of twenty-nine conclusions were drawn in the ten tests. For nineteen conclusions the submitted cartridge cases or bullets were either fired from the questioned firearm or from one and the same firearm, in tests where no firearm was submitted. For ten conclusions the submitted cartridge cases or bullets were either fired from another firearm than the submitted one or from several firearms, in tests where no firearm was submitted. In none of the conclusions misleading evidence was reported, in the sense that all conclusions supported the true hypothesis. This article discusses the design considerations of the program, contains details of the tests, and describes the various ways the test results were and could be analyzed.},
	pages = {514--519},
	number = {6},

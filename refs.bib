	note = {00000},
	keywords = {Forensic science, Random forest, 3D microscopy, cross correlation, land engraved areas ({LEAs}), random forest, Toolmark, Cross correlation, Land engraved areas ({LEAs})},
	file = {ScienceDirect Snapshot:/home/susan/Nextcloud/Zotero/storage/3F4XKHES/S0379073820300293.html:text/html;Vanderplas et al. - 2020 - Comparison of three similarity scores for bullet L.pdf:/home/susan/Nextcloud/Zotero/storage/6QZ7EESJ/Vanderplas et al. - 2020 - Comparison of three similarity scores for bullet L.pdf:application/pdf},
